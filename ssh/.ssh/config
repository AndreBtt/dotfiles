Host *
   # UseKeychain yes
   AddKeysToAgent yes
   IdentityFile ~/.ssh/id_rsa

# Kerberos options
# Ticket forwarding is enabled by default for csail machines
# It's not enabled globally because forwarding tickets to an
# untrusted system can be very bad.
# Enable for systems you trust.
Host *.csail.mit.edu
   GSSAPIAuthentication yes
   # GSSAPIKeyExchange yes
   GSSAPIDelegateCredentials yes
   # GSSAPITrustDNS yes
   # optional, if X forwarding is desired
   # ForwardX11 yes
   # optional, if your local username does not match YOUR_CSAIL_USERNAME
   User jjgo
   # Port Forwarding

Host night
   User jjgo
   GSSAPIAuthentication yes
   GSSAPIDelegateCredentials yes

   HostName night.csail.mit.edu
   # rmate for sublime text
   RemoteForward 52698 localhost:52698
   # HTTP Server
   LocalForward 9910 localhost:9910
   LocalForward 9920 localhost:9920
   # Tensorboard Server
   LocalForward 9916 localhost:9916
   LocalForward 9926 localhost:9926
   # Jupyter Notebook Server
   LocalForward 9918 localhost:9918
   LocalForward 9928 localhost:9928

Host ddmg
   User jjgo
   GSSAPIAuthentication yes
   GSSAPIDelegateCredentials yes

   HostName ddmg1.csail.mit.edu
   # rmate for sublime text
   RemoteForward 52698 localhost:52698
   # HTTP Server
   LocalForward 9910 localhost:9910
   LocalForward 9920 localhost:9920
   # Tensorboard Server
   LocalForward 9916 localhost:9916
   LocalForward 9926 localhost:9926
   # Jupyter Notebook Server
   LocalForward 9918 localhost:9918
   LocalForward 9928 localhost:9928
