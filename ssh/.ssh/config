Host *
   UseKeychain yes
   AddKeysToAgent yes
   IdentityFile ~/.ssh/id_rsa

Host lxplus.cern.ch
  user jjgonzal

Host INT-513-2-015.cern.ch
  ProxyCommand ssh lxplus.cern.ch /usr/bin/nc %h %p 2> /dev/null
# GSSAPITrustDNS no

Host opladev47.cern.ch
  ProxyCommand ssh lxplus.cern.ch /usr/bin/nc %h %p 2> /dev/null

Host Kartana.cern.ch
  ProxyCommand ssh lxplus.cern.ch /usr/bin/nc %h %p 2> /dev/null


# Kerberos options
# Ticket forwarding is enabled by default for csail machines
# It's not enabled globally because forwarding tickets to an
# untrusted system can be very bad.
# Enable for systems you trust.
Host *.csail.mit.edu
   GSSAPIAuthentication yes
   # GSSAPIKeyExchange yes
   GSSAPIDelegateCredentials yes
   # GSSAPITrustDNS yes
   # optional, if X forwarding is desired
   #ForwardX11 yes
   # optional, if your local username does not match YOUR_CSAIL_USERNAME
   User jjgo