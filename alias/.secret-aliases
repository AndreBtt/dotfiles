# Alias with plaintext passwords

alias ddmg="ssh -X -L 7777:localhost:8888 jjgo@ddmg1.csail.mit.edu"
alias joel="ssh -L 7778:127.0.0.1:8888 jjgo@128.30.77.111"

# OS dependent aliases
case "$(uname -s)" in

   Darwin)
     # echo 'Mac OS X'
     alias p7jj="cd /Volumes/Macintosh\ HD/Usuarios/JoseJavier/Series && mount -t smbfs //josejavier:curvycopter@p7jj/Descargas/KodiTemp ./KodiTemp &"
    alias popcorn="cd /Volumes/Macintosh\ HD/Usuarios/JoseJavier/Series && mount -t smbfs //nmt:1234@pch-a210/share ./PCH && cd -"

     ;;

   Linux)
     # echo 'Linux'
     ;;

   CYGWIN*|MINGW32*|MSYS*)
     # echo 'MS Windows'
     ;;

   # Add here more strings to compare
   # See correspondence table at the bottom of this answer

   *)
     # echo 'other OS'
     ;;
esac

